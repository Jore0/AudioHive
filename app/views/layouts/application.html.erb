<!DOCTYPE html>
<html>
  <head>
    <title>AudioHive</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
  <link href="https://fonts.googleapis.com/css?family=Roboto&display=swap" rel="stylesheet">
    <%= stylesheet_link_tag   'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <script type="text/javascript">
        window.soundcloud = "<%= image_url("soundcloud.png") %>";
        window.create = "<%= image_url("create.png") %>";
        window.login = "<%= image_url("login.png") %>";
        window.header1 = "<%= image_url("header1.jpg") %>";
        window.header2 = "<%= image_url("header2.jpg") %>";
        window.mini = "<%= image_url("miniSoundcloud.png") %>";

    </script>
    <%= yield %>
  </body>
</html>
